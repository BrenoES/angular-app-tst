{
  "name": "angular-app-tst",
  "version": "1.0.8",
  "description": "Angular 12 boilerplate that comes with Material-UI, Tailwind, Purgecss, Jest & Cypress Support, Optimal project structure, Interceptor with already some code, sourcemap analyzer tools, all pre-configured and much more...",
  "homepage": "https://github.com/BrenoES/angular-app-tst#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrenoES/angular-app-tst.git"
  },
  "keywords": [
    "angular",
    "boilerplate",
    "starter-kit",
    "material-template",
    "tailwind-template",
    "husky",
    "code-coverage",
    "light-house"
  ],
  "author": "Breno Silva <breno.esilva@outlook.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prepare": "husky install",
    "start": "node server.js",
    "start:local": "ng serve",
    "build": "ng build --base-href ./ ",
    "watch": "ng build --watch --configuration development",
    "prod-test": "npm run final-build && serve -s dist/",
    "heroku-postbuild": "ng build --prod",
    "source": "ng build --sourceMap=true --namedChunks=true && ./node_modules/.bin/source-map-explorer dist/*.js",
    "webpack-analyze": "ng build --sourceMap=true --namedChunks=true --stats-json && ./node_modules/.bin/webpack-bundle-analyzer dist/stats.json",
    "purgecss": "node run-purgecss.js",
    "final-build": "npm run build && npm run purgecss",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --colors --verbose",
    "lint": "ng lint",
    "e2e": "ng run ng-new-app:cypress-open",
    "e2e:ci": "ng run ng-new-app:cypress-run",
    "prettier": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:verify": "prettier --config ./.prettierrc --check \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:staged": "pretty-quick --staged"
  },
  "bin": {
    "ng-new-app": "./bin/generate-app.js"
  },
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@ngx-loading-bar/core": "^6.0.2",
    "@ngx-loading-bar/router": "^6.0.2",
    "@ngxs/storage-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "express": "^4.17.1",
    "json-server": "^0.17.0",
    "material-icons": "^1.10.0",
    "path": "^0.12.7",
    "rxjs": "^7.5.6",
    "tslib": "^2.0.0",
    "typed.js": "^2.0.11",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.0.0",
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@briebug/cypress-schematic": "^5.0.0",
    "@briebug/jest-schematic": "^4.0.0",
    "@cypress/code-coverage": "^3.9.9",
    "@schematics/angular": "^14.0.1",
    "@testing-library/angular": "^12.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.2.6",
    "cypress": "^10.3.0",
    "dayjs": "^1.10.5",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^28.0.4",
    "jest-preset-angular": "^12.2.0",
    "ngx-build-plus": "^14.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "purgecss": "^4.0.3",
    "rimraf": "^3.0.2",
    "serve": "^14.0.0",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.1.0",
    "typescript": "^4.6.4",
    "webpack-bundle-analyzer": "^4.4.1"
  },
  "engines": {
    "node": ">=v12.18.3",
    "npm": ">=6.14.6"
  }
}
